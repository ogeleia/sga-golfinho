<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class            name="br.com.sga.site.bean.GrupoModulo"
                   table="GrupoModulo">

    <!-- Primary Key -->
    <id             name="codigousuario"  column="codigousuario">
      <generator   class="increment"/>
    </id>

    <!-- Direct Data Properties -->
    <property  name="codigogrupo"    column="codigogrupo"/>
  </class>

</hibernate-mapping>