<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class            name="br.com.sga.site.bean.Historico"
                   table="Historico">

    <!-- Primary Key -->
    <id             name="codigohistorico"  column="codigohistorico">
      <generator   class="increment"/>
    </id>

    <!-- Direct Data Properties -->
    <property  name="codigo" column="codigo" />       
    <property  name="data"        column="data"/>
    <property  name="tipo"        column="tipo"/>
    <property  name="ocorrencia"  column="ocorrencia"/>

  </class>

</hibernate-mapping>