<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

  <class            name="br.com.sga.site.bean.Matricula"
                   table="Matricula">

    <!-- Primary Key -->
    <id             name="codigo"  column="codigo">
      <generator   class="increment"/>
    </id>

    <!-- Direct Data Properties -->
    <property  name="codigoaluno"      column="codigoaluno"/>
    <property  name="codigoturma"      column="codigoturma"/>
    <property  name="codigoplano"      column="codigoplano"/>
    <property  name="status"           column="status"/>
    <property  name="valormensalidade" column="valormensalidade"/>
    
  </class>

</hibernate-mapping>